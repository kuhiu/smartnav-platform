{
  "fuzzy_system": {

    "inputs": 
    [
      {
        "name": "left_distance_sensor",
        "membership_functions": 
        [
          { "name": "FAR", "trapezoidal": [{"x1": 0}, {"x2": 1000}, {"right_slope": 0.0111}, {"left_slope": 0.0111}] },
          { "name": "NEAR",  "trapezoidal": [{"x1": -90}, {"x2": 90}, {"right_slope": 0.0111}, {"left_slope": 0.0111}] } 
        ]
      },
      {
        "name": "center_distance_sensor",
        "membership_functions": 
        [
          { "name": "FAR", "trapezoidal": [{"x1": 0}, {"x2": 1000}, {"right_slope": 0.0111}, {"left_slope": 0.0111}] },
          { "name": "NEAR", "trapezoidal": [{"x1": -90}, {"x2": 90}, {"right_slope": 0.0111}, {"left_slope": 0.0111}] }
        ]
      },
      { 
        "name": "right_distance_sensor",
        "membership_functions": 
        [
          { "name": "FAR", "trapezoidal": [{"x1": 0}, {"x2": 1000}, {"right_slope": 0.0111}, {"left_slope": 0.0111}] },
          { "name": "NEAR", "trapezoidal": [{"x1": -90}, {"x2": 90}, {"right_slope": 0.0111}, {"left_slope": 0.0111}] }
        ]
      },
      { 
        "name": "lighting",
        "membership_functions": 
        [
          { "name": "HIGH", "trapezoidal": [{"x1": 50}, {"x2": 150}, {"right_slope": 0.02}, {"left_slope": 0.02}] },
          { "name": "MEDIUM", "trapezoidal": [{"x1": 0}, {"x2": 100}, {"right_slope": 0.02}, {"left_slope": 0.02}] },
          { "name": "LOW", "trapezoidal": [{"x1": -50}, {"x2": 50}, {"right_slope": 0.02}, {"left_slope": 0.02}] }
        ]
      }
    ], 

    "outputs": 
    [
      {
        "name": "heading",
        "membership_functions": 
        [
          { "name": "RIGHT", "trapezoidal": [{"x1": 0}, {"x2": 10}, {"right_slope": 0.2}, {"left_slope": 0.2}] },
          { "name": "CENTER", "trapezoidal": [{"x1": -5}, {"x2": 5}, {"right_slope": 0.2}, {"left_slope": 0.2}] },
          { "name": "LEFT", "trapezoidal": [{"x1": -10}, {"x2": 0}, {"right_slope": 0.2}, {"left_slope": 0.2}] }
        ]
      },
      {
        "name": "car_headlights",
        "membership_functions": 
        [
          { "name": "HIGH", "trapezoidal": [{"x1": 50}, {"x2": 150}, {"right_slope": 0.02}, {"left_slope": 0.02}] },
          { "name": "MEDIUM", "trapezoidal": [{"x1": 0}, {"x2": 100}, {"right_slope": 0.02}, {"left_slope": 0.02}] },
          { "name": "LOW", "trapezoidal": [{"x1": -50}, {"x2": 50}, {"right_slope": 0.02}, {"left_slope": 0.02}] }
        ]
      },
      {
        "name": "car_speed",
        "membership_functions": 
        [
          { "name": "INCREASE", "trapezoidal": [{"x1": 0}, {"x2": 10}, {"right_slope": 0.2}, {"left_slope": 0.2}] },
          { "name": "SAME", "trapezoidal": [{"x1": -5}, {"x2": 5}, {"right_slope": 0.2}, {"left_slope": 0.2}] },
          { "name": "REDUCE", "trapezoidal": [{"x1": -10}, {"x2": 0}, {"right_slope": 0.2}, {"left_slope": 0.2}] }
        ]
      }
    ],

    "rules": 
    [
      { 
        "name": "rule_0", 
        "input_conditions": [{ "and": [ { "io": "left_distance_sensor", "fuzzy_value": "FAR" }, 
                                        { "io": "center_distance_sensor", "fuzzy_value": "FAR" }, 
                                        { "io": "right_distance_sensor", "fuzzy_value": "FAR" } ] }], 
        "output_conditions": [{ "and": [ { "io": "heading", "fuzzy_value": "CENTER" }, 
                                         { "io": "car_speed", "fuzzy_value": "INCREASE"} ] }] 
      },
      { 
        "name": "rule_1",
        "input_conditions": [{ "and": [ { "io": "left_distance_sensor", "fuzzy_value": "FAR" }, 
                                        { "io": "center_distance_sensor", "fuzzy_value": "FAR" }, 
                                        { "io": "right_distance_sensor", "fuzzy_value": "NEAR" } ] }], 
        "output_conditions": [{ "and": [ { "io": "heading", "fuzzy_value": "LEFT" },
                                         { "io": "car_speed", "fuzzy_value": "SAME"} ] }] 
      },
      { 
        "name": "rule_2",
        "input_conditions": [{ "and": [ { "io": "left_distance_sensor", "fuzzy_value": "FAR" }, 
                                        { "io": "center_distance_sensor", "fuzzy_value": "NEAR" }, 
                                        { "io": "right_distance_sensor", "fuzzy_value": "FAR" } ] }], 
        "output_conditions": [{ "and": [ { "io": "heading", "fuzzy_value": "LEFT" },
                                         { "io": "car_speed", "fuzzy_value": "REDUCE"} ] }]  
      },
      { 
        "name": "rule_3",
        "input_conditions": [{ "and": [ { "io": "left_distance_sensor", "fuzzy_value": "FAR" }, 
                                        { "io": "center_distance_sensor", "fuzzy_value": "NEAR" }, 
                                        { "io": "right_distance_sensor", "fuzzy_value": "NEAR" } ] }], 
        "output_conditions": [{ "and": [ { "io": "heading", "fuzzy_value": "LEFT" },
                                         { "io": "car_speed", "fuzzy_value": "REDUCE"} ] }] 
      },
      { 
        "name": "rule_4",
        "input_conditions": [{ "and": [ { "io": "left_distance_sensor", "fuzzy_value": "NEAR" }, 
                                        { "io": "center_distance_sensor", "fuzzy_value": "FAR" }, 
                                        { "io": "right_distance_sensor", "fuzzy_value": "FAR" } ] }], 
        "output_conditions": [{ "and": [ { "io": "heading", "fuzzy_value": "RIGHT" },
                                         { "io": "car_speed", "fuzzy_value": "SAME"} ] }] 
      },
      { 
        "name": "rule_5",
        "input_conditions": [{ "and": [ { "io": "left_distance_sensor", "fuzzy_value": "NEAR" }, 
                                        { "io": "center_distance_sensor", "fuzzy_value": "FAR" }, 
                                        { "io": "right_distance_sensor", "fuzzy_value": "NEAR" } ] }], 
        "output_conditions": [{ "and": [ { "io": "heading", "fuzzy_value": "CENTER" },
                                         { "io": "car_speed", "fuzzy_value": "SAME"} ] }]  
      },
      { 
        "name": "rule_6",
        "input_conditions": [{ "and": [ { "io": "left_distance_sensor", "fuzzy_value": "NEAR" }, 
                                        { "io": "center_distance_sensor", "fuzzy_value": "NEAR" }, 
                                        { "io": "right_distance_sensor", "fuzzy_value": "FAR" } ] }], 
        "output_conditions": [{ "and": [ { "io": "heading", "fuzzy_value": "RIGHT" },
                                         { "io": "car_speed", "fuzzy_value": "REDUCE"} ] }] 
      },
      { 
        "name": "rule_7",
        "input_conditions": [{ "and": [ { "io": "left_distance_sensor", "fuzzy_value": "NEAR" }, 
                                        { "io": "center_distance_sensor", "fuzzy_value": "NEAR" }, 
                                        { "io": "right_distance_sensor", "fuzzy_value": "NEAR" } ] }], 
        "output_conditions": [{ "and": [ { "io": "heading", "fuzzy_value": "LEFT" },
                                         { "io": "car_speed", "fuzzy_value": "REDUCE"} ] }]   
      },
      { 
        "name": "rule_8",
        "input_conditions": [ { "io": "lighting", "fuzzy_value": "LOW" }], 
        "output_conditions": [ { "io": "car_headlights", "fuzzy_value": "HIGH" } ] 
      },
      { 
        "name": "rule_9",
        "input_conditions": [ { "io": "lighting", "fuzzy_value": "MEDIUM" }], 
        "output_conditions": [ { "io": "car_headlights", "fuzzy_value": "MEDIUM" } ] 
      },
      { 
        "name": "rule_10",
        "input_conditions": [ { "io": "lighting", "fuzzy_value": "HIGH" }], 
        "output_conditions": [ { "io": "car_headlights", "fuzzy_value": "LOW" } ] 
      }
    ]
  } 
}